FUNCTION
   _fnsTextMessageByIndex(dwMsgIndex : System.Word) : STRING;
{*
Description.
   Extracts a text message from a table if specified index in valid range;
   displays a 'bad index' message if bad index found.
*}
BEGIN
   IF  ((dwMsgIndex > aTableIndexMax)  OR
        (dwMsgIndex < aTableIndexMin))
     THEN  dwMsgIndex := aTableIndexMin;
   {if-then}

   _fnsTextMessageByIndex := Strings.StrPas(gszMsgIdxTable[Globals.gdwCurLanguage,dwMsgIndex]);
END;
